{
  "name": "indoor-golf-scheduler",
  "version": "1.0.0",
  "description": "TypeScript-based digital indoor golf scheduling system with advanced features",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production vite build --mode production",
    "build:dev": "NODE_ENV=development vite build --mode development",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "build:analyze": "NODE_ENV=production vite build --mode production && npx vite-bundle-analyzer dist",
    "test": "jest",
    "test:verbose": "VERBOSE_TESTS=true jest",
    "test:quiet": "QUIET_TESTS=true jest",
    "test:ci": "CI=true jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:verbose": "VERBOSE_TESTS=true playwright test",
    "test:e2e:quiet": "QUIET_TESTS=true playwright test",
    "test:e2e:ci": "CI=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "dev": "vite",
    "clean": "rm -rf dist",
    "serve": "vite preview",
    "start": "npm run build && npm run serve",
    "dev:server": "npm run build && node server.js",
    "preview": "vite preview --port 3000",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit --skipLibCheck"
  },
  "keywords": [
    "golf",
    "scheduler",
    "indoor",
    "sports"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.0.0",
    "@types/papaparse": "^5.5.2",
    "fast-check": "^3.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.3.0",
    "jspdf": "^4.0.0",
    "papaparse": "^5.5.3",
    "terser": "^5.44.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.0",
    "webpack-cli": "^6.0.1"
  }
}
